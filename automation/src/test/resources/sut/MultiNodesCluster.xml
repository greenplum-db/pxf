<sut>
	<cluster>
		<class>org.greenplum.pxf.automation.components.cluster.MultiNodeCluster</class>
		<host>localhost</host>
		<phdRoot>/usr/local/greenplum-db-devel</phdRoot>
		<hiveBaseHdfsDirectory>/hive/warehouse/</hiveBaseHdfsDirectory>
		<nodes index="0">
			<class>org.greenplum.pxf.automation.components.cluster.installer.nodes.MasterNode</class>
			<host>mdw</host>
			<userName>gpadmin</userName>
			<password></password>
			<services>gpdb,pxf</services>
		</nodes>
		<nodes index="1">
			<class>org.greenplum.pxf.automation.components.cluster.installer.nodes.SlaveNode</class>
			<host>sdw1</host>
			<userName>gpadmin</userName>
			<password></password>
			<services>pxf</services>
		</nodes>
		<nodes index="2">
			<class>org.greenplum.pxf.automation.components.cluster.installer.nodes.SlaveNode</class>
			<host>sdw2</host>
			<userName>gpadmin</userName>
			<password></password>
			<services>pxf</services>
		</nodes>
	</cluster>

	<gpdb>
		<class>org.greenplum.pxf.automation.components.gpdb.Gpdb</class>
		<host>localhost</host>
		<masterHost>mdw</masterHost>
		<userName>gpadmin</userName>
		<db>pxfautomation</db>
	</gpdb>
	<hdfs>
		<class>org.greenplum.pxf.automation.components.hdfs.Hdfs</class>
		<host>hadoop</host>
		<port>8020</port>
		<workingDirectory>tmp/pxf_automation_data/__UUID__</workingDirectory>
		<haNameservice></haNameservice>
	</hdfs>
	<hbase>
		<class>org.greenplum.pxf.automation.components.hbase.HBase</class>
		<host>hadoop</host>
		<userName>gpadmin</userName>
		<hbaseRoot>/etc/hbase</hbaseRoot>
	</hbase>
	<hive>
		<class>org.greenplum.pxf.automation.components.hive.Hive</class>
		<host>hadoop</host>
		<port>10000</port>
		<userName>gpadmin</userName>
	</hive>
	<file>
		<class>org.greenplum.pxf.automation.components.hdfs.Hdfs</class>
		<workingDirectory>${base.path}/tmp/pxf_automation_data/__UUID__</workingDirectory>
		<hadoopRoot>${pxf.base}/servers/file</hadoopRoot>
		<scheme>file</scheme>
	</file>
	<tinc>
		<class>org.greenplum.pxf.automation.components.tinc.Tinc</class>
		<host>localhost</host>
		<userName>gpadmin</userName>
		<gphome>/usr/local/greenplum-db-devel</gphome>
		<tincFolder>tinc/main</tincFolder>
		<tincTestsFolder>tincrepo/main</tincTestsFolder>
	</tinc>
	<pxf>
		<class>org.greenplum.pxf.automation.components.pxf.Pxf</class>
		<host>localhost</host>
		<port>5888</port>
	</pxf>

	<shellsystemobject>
		<JAVA_HOME></JAVA_HOME>
		<GPHOME></GPHOME>
		<GPHD_ROOT></GPHD_ROOT>
		<GPDATA></GPDATA>
		<MASTER_DATA_DIRECTORY></MASTER_DATA_DIRECTORY>
		<PGPORT></PGPORT>
		<PGHOST></PGHOST>
		<PGDATABASE></PGDATABASE>
	</shellsystemobject>
</sut>
