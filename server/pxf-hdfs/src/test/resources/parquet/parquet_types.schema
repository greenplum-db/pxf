message hive_schema {
  OPTIONAL INT32 id;
  OPTIONAL BYTE_ARRAY name (UTF8);
  OPTIONAL INT32 cdate (DATE);
  OPTIONAL DOUBLE amt;
  OPTIONAL BYTE_ARRAY grade (UTF8);
  OPTIONAL BOOLEAN b;
  OPTIONAL INT96 tm;
  OPTIONAL INT64 bg;
  OPTIONAL BYTE_ARRAY bin;
  OPTIONAL INT32 sml (INT_16);
  OPTIONAL FLOAT r;
  OPTIONAL BYTE_ARRAY vc1 (UTF8);
  OPTIONAL BYTE_ARRAY c1 (UTF8);
  OPTIONAL FIXED_LEN_BYTE_ARRAY (16) dec1 (DECIMAL18, 38);
  OPTIONAL FIXED_LEN_BYTE_ARRAY (3) dec2 (DECIMAL2, 5);
  OPTIONAL FIXED_LEN_BYTE_ARRAY (6) dec3 (DECIMAL5, 13);
  OPTIONAL INT32 num1;
}
